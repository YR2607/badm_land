{
  "rewrites": [
    { "source": "/admin", "destination": "/admin/index.html" },
    { "source": "/admin/(.*)", "destination": "/admin/index.html" },
    { "source": "/static/(.*)", "destination": "/admin/static/$1" },
    { "source": "/vendor/(.*)", "destination": "/admin/vendor/$1" },
    { "source": "/manifest.webmanifest", "destination": "/admin/manifest.webmanifest" },
    { "source": "/favicon.ico", "destination": "/admin/favicon.ico" },
    { "source": "/favicon.svg", "destination": "/admin/favicon.svg" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "cleanUrls": true
}
