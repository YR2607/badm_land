{
  "git": {
    "deploymentEnabled": {
      "preview": false,
      "production": false
    }
  },
  "ignoreCommand": "exit 0",
  "routes": [
    { "handle": "filesystem" },
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "src": "^/data/(.*)$", "dest": "/data/$1" },
    { "src": "^/assets/(.*)$", "dest": "/assets/$1" },
    { "src": "^/static/(.*)$", "dest": "/admin/static/$1" },
    { "src": "^/vendor/(.*)$", "dest": "/admin/vendor/$1" },
    { "src": "^/manifest\\.webmanifest$", "dest": "/admin/manifest.webmanifest" },
    { "src": "^/favicon\\.(ico|svg)$", "dest": "/admin/favicon.$1" },
    { "src": "^/admin$", "dest": "/admin/index.html" },
    { "src": "^/admin/.*$", "dest": "/admin/index.html" },
    { "src": ".*", "dest": "/index.html" }
  ],
  "crons": [
    { "path": "/api/bwf-news?refresh=1", "schedule": "0 5 * * *" }
  ]
}
